function printDate() {
        var date = new Date();
        var day  = date.getDate();
        var month = date.getMonth() + 1;
        var year = date.getYear();
        year = (year < 1000) ? year + 1900 : year;

        document.write(year + "."+ month + "." + day + "")
}
