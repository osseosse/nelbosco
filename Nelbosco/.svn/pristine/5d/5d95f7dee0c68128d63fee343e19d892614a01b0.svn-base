.gnbBG {background:#fff url('../img/gnb_bg.gif')  repeat-x; background-position:0px 0px; display:none; position:absolute; width:100%; background-color:rgba( 0, 0, 0, 0.55 );top:89px; left:0; height:400px;  }
.brown {color:#b17947; }
.grayline{border-bottom: 2px solid #e1e1e1; padding-bottom: 120px;}
#gnb {left: 0px; top: 0px; width: 100%; /*position: fixed;*/ z-index:10; background-color:#fff; height:89px; z-index:10000; background-color:rgba( 0, 0, 0, 0.7 );  }
#gnb ul li ul {display:none;width:206px;height:89px; top:51px;left:0px; padding:8px 0px 7px 8px;/*background:#fff;*/ z-index:100;   }
/* 핫스팟 없을시 #gnb {position:relative;height:89px; width:100%; margin:0 auto; position:fixed; top:0  }*/
/* 핫스팟 적용시 */ #gnb {position:relative;height:89px; width:100%; margin:0 auto; position:absolute; top:150px  }

#gnb:after {display:block; content:"";clear:both;}
#gnb li {position:relative;float:left; top:0; }
#gnb li a.dep01{font-size:18px;font-weight:400;color:#fff;display:block;padding:34px 20px 34px 0px;text-align:center;margin-left:1px;text-align:left; line-height:120% ; }
#gnb li.last a.dep01 {width:200px; }
